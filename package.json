{
  "name": "@computas/nx-yarn",
  "version": "1.0.0-alpha1",
  "main": "src/index.js",
  "executors": "./executors.json",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "cpy './{package.json,README.md,executors.json}' ./dist/ && cpy ./src/**/{*.d.ts,*.json} ./dist",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@angular-devkit/schematics": "^13.3.6",
    "@nrwl/devkit": "^14.1.9",
    "@nrwl/workspace": "^14.1.9",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "nx": "^14.1.9",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "lint-staged": {
    "{src,.}/*.{js,ts,json,md}": [
      "prettier --config .prettierrc --write",
      "eslint"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
